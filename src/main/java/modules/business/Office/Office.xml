<?xml version="1.0" encoding="UTF-8"?>
<document xmlns="http://www.skyve.org/xml/document" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="Office" xsi:schemaLocation="http://www.skyve.org/xml/document ../../../schemas/document.xsd">
    <persistent name="BUS_Office"/>
    <singularAlias>Office</singularAlias>
    <pluralAlias>Offices</pluralAlias>
    <bizKey expression="Office - {levelUnit}"/>
    <attributes>
        <text name="levelUnit">
            <displayName>levelUnit</displayName>
            <length>200</length>
        </text>
        <text name="buildingName">
            <displayName>buildingName</displayName>
            <length>200</length>
        </text>
        <text name="streetAddress">
            <displayName>streetAddress</displayName>
            <length>200</length>
        </text>
        <text name="suburb">
            <displayName>suburb</displayName>
            <length>200</length>
        </text>
        <text name="postCode">
            <displayName>postCode</displayName>
            <length>8</length>
        </text>
        <text name="phone">
            <displayName>phone</displayName>
            <length>14</length>
        </text>
        <inverseMany name="officeStaff">
            <displayName>Staff</displayName>
            <documentName>Staff</documentName>
            <referenceName>baseOffice</referenceName>
        </inverseMany>
    </attributes>

</document>