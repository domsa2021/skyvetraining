<?xml version="1.0" encoding="UTF-8"?>
<view xmlns="http://www.skyve.org/xml/view" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="edit" title="Office" xsi:schemaLocation="http://www.skyve.org/xml/view ../../../../schemas/view.xsd">
    <form responsiveWidth="12" percentageWidth="60" border="true">
        <column percentageWidth="30" responsiveWidth="4"/>
        <column/>
        <row>
            <item>
                <default binding="levelUnit"/>
            </item>
        </row>
        <row>
            <item>
                <default binding="buildingName"/>
            </item>
        </row>
        <row>
            <item>
                <default binding="streetAddress"/>
            </item>
        </row>
        <row>
            <item>
                <default binding="suburb"/>
            </item>
        </row>
        <row>
            <item>
                <default binding="postCode"/>
            </item>
        </row>
        <row>
            <item>
                <default binding="phone"/>
            </item>
        </row>
        <row>
            <item>
                <default binding="boundary"/>
            </item>
        </row>
        <row>
            <item>
                <default binding="memo"/>
            </item>
        </row>
    </form>
    <dataGrid binding="officeStaff">
        <boundColumn binding="name" />
        <boundColumn binding="code" />
        <boundColumn binding="dateOfBirth" />
    </dataGrid>
    <map modelName="MyMapModel"></map>
    <actions>
        <defaults/>
    </actions>
    <newParameters/>
</view>
